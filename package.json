{
  "name": "webpack4-vue-typescript-cli",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "npm run start",
    "start": "webpack-dev-server --config ./build/webpack.dev.js --mode=development --env.dev=development --open --progress --parameters",
    "compile": "webpack-cli --progress --config ./build/webpack.prod.js --mode=production --env.dev=production --parameters",
    "analyzer": "webpack-cli --progress --config ./build/webpack.prod.js --mode=production --env.dev=production --env.dev=analyzer --profile --json > stats.json",
    "dll": "webpack --progress --colors --config ./build/webpack.dll.js",
    "lint": "eslint './src/**/*.{ts,vue,js}' --quiet --fix",
    "generate:vue": "node ./scripts/generate-vue/index"
  },
  "author": "",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "es-commit-msg-validator"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,vue,js}": [
      "npm eslint",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "add-asset-html-webpack-plugin": "^3.1.3",
    "autoprefixer": "^9.8.0",
    "babel-loader": "^8.1.0",
    "chalk": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.2",
    "css-loader": "^3.5.3",
    "eslint": "^6.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^6.2.2",
    "file-loader": "^6.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "happypack": "^5.0.1",
    "html-loader": "^1.1.0",
    "husky": "^4.2.5",
    "es-commit-msg-validator": "1.0.2",
    "less": "^3.11.3",
    "less-loader": "^6.1.1",
    "lint-staged": "^10.2.11",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss": "^7.0.32",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "2.3.6",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5",
    "url-loader": "^4.1.0",
    "vue-loader": "^15.9.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.10.1",
    "axios": "^0.19.2",
    "browserslist": "^4.16.5",
    "html-webpack-plugin": "^4.3.0",
    "html-withimg-loader": "^0.1.16",
    "ts-import-plugin": "^1.6.6",
    "vant": "^2.8.7",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^9.0.0",
